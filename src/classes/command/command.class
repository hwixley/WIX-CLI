#!/bin/bash

# Class named "command" for bash Object

. "$(pwd)/src/classes/lib/lib.h"

lib lib

# property
command_properties=()

# properties IDs
id=0

command.sayHello(){
    echo Hello
}

command.property(){
    if [ "$2" == "=" ]
    then
	    command_properties[$1]=$3
    else
	    echo "${command_properties[$1]}"
    fi
}

command.id(){
    if [ "$1" == "=" ]
    then
	    command.property id = "$2"
    else
	    command.property id
    fi
}


command.name(){
    lib.csv.name "$(command.id '')"
}

command.path(){
    lib.csv.path "$(command.id '')"
}