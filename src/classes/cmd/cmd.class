

. "${WIX_DIR}/src/classes/lib/lib.h"
lib lib

WIX_cmd_properties="id:default,name:default,path:default,public:default,args:default"
export WIX_cmd_properties

cmd.property(){
    if [ "$2" = "=" ]
    then
        WIX_cmd_properties=$(echo "$WIX_cmd_properties" | sed -E -e "s/$1:[^,]*/$1:$3/g")
    else
	    echo "${WIX_cmd_properties[$1]}"
    fi
}

cmd.id(){
    if [ "$1" = "=" ]
    then
	    cmd.property id = "$2"
    else
	    cmd.property id
    fi
}

cmd.name(){
    lib.csv.name "$(cmd.id '')"
}

cmd.path(){
    lib.csv.path "$(cmd.id '')"
}

cmd.public(){
    lib.csv.public "$(cmd.id '')"
}

cmd.args(){
    lib.csv.args "$(cmd.id '')"
}